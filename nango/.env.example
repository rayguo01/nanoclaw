# Nango Configuration
# Copy this to .env and fill in the values

# Get these from https://app.nango.dev/account-settings
NANGO_SECRET_KEY=your-secret-key
NANGO_PUBLIC_KEY=your-public-key

# Your domain (must be publicly accessible for OAuth callbacks)
NANGO_SERVER_URL=https://your-domain.com/nango
NANGO_CALLBACK_URL=https://your-domain.com/nango/oauth/callback

# Database password (change in production)
NANGO_DB_PASSWORD=nango
